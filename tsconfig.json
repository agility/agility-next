{
	"compilerOptions": {

	  "allowJs": true,
	  "declaration": true,
	  "jsx": "react",
	  "target": "es5",
	  "outDir": "./dist/",
	  "lib": ["es2016", "dom"],
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "allowSyntheticDefaultImports": true
	},
	"include": [
		"./src/*.ts",
		"./src/*.tsx",
		"./src/*.js"	]
  }